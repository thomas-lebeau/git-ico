{
  "name": "git-ico",
  "version": "1.0.3",
  "description": "quickie boilerplate",
  "scripts": {
    "start": "parcel ./src/index.jsx --out-dir ./bin --target node",
    "lint": "eslint ./src",
    "size": "size-limit",
    "build": "parcel build ./src/index.jsx --target node --out-dir ./bin --bundle-node-modules --no-source-maps",
    "release": "npm publish",
    "clean": "rimraf ./bin",
    "postbuild": "npm run size",
    "prebuild": "npm run clean && npm run lint"

  },
  "author": {
    "name": "Thomas Lebeau",
    "email": "lebeau.thomas@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomas-lebeau/git-ico.git"
  },
  "license": "MIT",
  "bin": {
    "git-ico": "./bin/index.js"
  },
  "engines": {
    "node": ">=8"
  },
  "files": [
    "bin"
  ],
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.20.3",
    "npm-publish-git-tag": "^3.0.4",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-shebang": "^1.2.3",
    "prettier": "^1.16.4",
    "semantic-release-github": "^5.0.1",
    "size-limit": "^0.22.0"
  },
  "dependencies": {
    "ink": "^2.7.1",
    "ink-select-input": "^3.1.2",
    "ink-text-input": "^3.3.0",
    "meow": "^5.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6"
  },
  "size-limit": [
    {
      "path": [
        "./bin/*.js"
      ],
      "limit": "1 MB",
      "webpack": false,
      "gzip": false
    }
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "root": true,
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "react/prop-types": [
        "error",
        {
          "ignore": "children"
        }
      ]
    },
    "parserOptions": {
      "ecmaVersion": 2019,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    }
  },
  "eslintIgnore": [
    "bin/"
  ],
  "bugs": {
    "url": "https://github.com/thomas-lebeau/git-ico/issues"
  },
  "readme": "ERROR: No README data found!",
  "homepage": "https://github.com/thomas-lebeau/git-ico#readme",
  "_id": "git-ico@0.0.0"
}
